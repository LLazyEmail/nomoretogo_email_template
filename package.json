{
  "name": "nmtg-template-mailerlite",
  "version": "0.0.2",
  "description": "im trying to separate part of our code",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "scripts": {
    "bundle": "rollup -c",
    "dev": "rollup -c -w",
    "build": "npm run clean && npm run bundle",
    "build:babel": "babel src -d dist",
    "prepublish": "npm run clean && npm run build",
    "test:all": "jest ./tests/* --passWithNoTests",
    "test:content": "bash ./bash/tests.sh 03_content",
    "test:recipes": "bash ./bash/tests.sh 03_recipes",
    "test:typo": "npm run chmodler && ./bash/05-test-typography.sh",
    "test:new": "npm run chmodler && ./bash/tests.sh",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "chmodler": "shx chmod -R 755 ./bash",
    "lint:prettier": "prettier --write src/",
    "upd": "shx chmod +x ./bash/update-packages.sh && ./bash/update-packages.sh",
    "lint": "eslint --ext .js . --ignore-path ./.eslintignore --resolve-plugins-relative-to .",
    "lint:fix": "shx chmod +x ./bash/lint-fix.sh && ./bash/lint-fix.sh",
    "lint2:fix": "eslint --fix . --ignore-path ./.eslintignore --resolve-plugins-relative-to ."
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "eslint": "8.26.0",
    "eslint-plugin-json": "3.1.0",
    "jest": "29.2.2",
    "markup-generator": "1.3.0",
    "prettier": "2.7.1",
    "rollup": "3.3.0",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "semver": "7.3.8",
    "shx": "0.3.4"
  },
  "author": "Arthur Tkachenko",
  "license": "MIT",
  "private": false,
  "release": {
    "publishDir": "dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "babel-preset-react-app": "^10.0.1",
    "email-template-object": "^0.2.0",
    "nmtg-template-mailerlite-inner": "^2.3.0",
    "nmtg-template-mailerlite-miscellaneous": "^0.10.0",
    "nmtg-template-mailerlite-outertemplate": "^2.3.0",
    "nmtg-template-mailerlite-typography": "^3.8.0"
  }
}
